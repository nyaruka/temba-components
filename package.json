{
  "name": "@nyaruka/temba-components",
  "version": "0.135.5",
  "description": "Web components to support rapidpro and related projects",
  "author": "Nyaruka <code@nyaruka.coim>",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "homepage": "https://github.com/nyaruka/temba-components/",
  "license": "AGPL-3.0-only",
  "repository": "https://github.com/nyaruka/temba-components/",
  "scripts": {
    "start": "concurrently --kill-others --names tsc,web-dev-server \"yarn tsc:watch\" \"web-dev-server --app-index demo/index.html --node-resolve --watch --port 3010 --esbuild-target auto\"",
    "tsc:watch": "tsc --watch",
    "build": "rimraf dist && yarn svg && tsc && rollup -c rollup.components.mjs ",
    "build-wc": "rollup -c rollup.webchat.mjs",
    "dev": "yarn build && cp -R ./dist/* ../temba/rapidpro/node_modules/@nyaruka/temba-components/dist/ && cp -R ./dist/* ../floweditor/node_modules/@nyaruka/temba-components/dist/",
    "postversion": "git push --tags && git push origin main",
    "pre-commit": "yarn locale:extract && yarn locale:build && yarn svg && git add ./src/Icons.ts ./static/svg/index.svg ./xliff ./src/locales && lint-staged",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" \"**/*.ts\" --config .prettierrc --write --ignore-path .gitignore",
    "format": "yarn format:eslint && yarn format:prettier",
    "test": "wtr --node-resolve",
    "test:fast": "wtr --node-resolve --fast",
    "test:coverage": "wtr --node-resolve --coverage",
    "check-coverage": "node check-coverage.js",
    "validate": "yarn format && yarn build && yarn test:coverage && yarn check-coverage",
    "test:watch": "wtr --node-resolve --watch",
    "stress-test": "node stress-test.js",
    "svg": "rimraf static/svg/work && node svg.js --resolution=150 --output='./static/svg/index.svg' --usage='./src/Icons.ts'",
    "svg-wc": "rimraf static/svg/work && node svg.js --resolution=150 --output='./static/svg/webchat.svg' --usage='./src/webchat/index.ts'",
    "version": "yarn run build && auto-changelog -p && git add CHANGELOG.md",
    "locale:extract": "lit-localize extract --config localize.config.json",
    "locale:build": "echo 'Building localizations..' && lit-localize build --config localize.config.json > dist/locale.out 2>&1",
    "dev-data:sync": "node scripts/dev-data-sync.mjs copy",
    "dev-data:reset": "node scripts/dev-data-sync.mjs reset",
    "dev-data:wipe": "node scripts/dev-data-sync.mjs reset",
    "dev-data:status": "node scripts/dev-data-sync.mjs status"
  },
  "dependencies": {
    "@jsplumb/browser-ui": "^6.2.10",
    "@lit/localize": "^0.12.1",
    "@open-wc/lit-helpers": "^0.7.0",
    "chart.js": "^4.4.8",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "color-hash": "^2.0.2",
    "croppie": "^2.6.5",
    "geojson": "^0.5.0",
    "image-size": "^1.1.1",
    "immer": "10",
    "leaflet": "1.5.1",
    "lit": "3.1.2",
    "lit-element": "^4.0.4",
    "lit-html": "^3.1.2",
    "luxon": "3.5",
    "remarkable": "^2.0.1",
    "serialize-javascript": "^6.0.2",
    "tiny-lru": "^11.2.5",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@lit/localize-tools": "^0.7.2",
    "@open-wc/building-rollup": "3.0.2",
    "@open-wc/eslint-config": "12.0.3",
    "@open-wc/testing": "4.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/leaflet": "1.4.4",
    "@types/node": "20.12.4",
    "@types/remarkable": "^2.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@web/dev-server-esbuild": "^0.3.3",
    "@web/test-runner": "0.15.0",
    "@web/test-runner-puppeteer": "^0.11.0",
    "auto-changelog": "*",
    "busboy": "^1.6.0",
    "concurrently": "^7.6.0",
    "dynamicpixelmatch": "^0.0.2",
    "eslint": "^8.29.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lit": "^1.10.1",
    "eslint-plugin-lit-a11y": "^4.1.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-wc": "^1.2.0",
    "fprint": "^2.0.1",
    "htmlclean": "^3.0.8",
    "husky": "^1.0.0",
    "lint-staged": "^10.0.0",
    "minio": "^8.0.5",
    "node-fetch": "^3.3.2",
    "oslllo-svg-fixer": "^2.2.0",
    "pixelmatch": "^5.2.1",
    "prettier": "^2.0.4",
    "rimraf": "^2.6.3",
    "rollup": "^3.21.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-svg-import": "^3.0.0",
    "rollup-plugin-url": "^3.0.1",
    "shelljs": "^0.8.5",
    "sinon": "^21.0.0",
    "svgstore": "^3.0.1",
    "tslib": "2.6.2",
    "typescript": "5.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --config .prettierrc --write",
      "eslint"
    ]
  },
  "resolutions": {
    "trim": "^0.0.3",
    "prismjs": "^1.23.0",
    "rollup-plugin-terser/**/rollup": "^3.0.0"
  },
  "packageManager": "yarn@1.22.1"
}
